{"ast":null,"code":"import { request } from '@/utils/request';\nexport default {\n  name: 'HomeView',\n  components: {},\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: \"\",\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        //console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        request.put(\"/user\", this.form).then(res => {\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n        });\n      } else {\n        request.post(\"/user\", this.form).then(res => {\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n        });\n      }\n      this.load();\n      this.dialogVisible = false;\n    },\n    handleEdit(row) {\n      //深拷贝，为了防止更改了本地变量里面的form\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleSizeChange(pageSize) {\n      //改变当前每页的个数触发\n      //this.pageSize = pageSize; 我使用了双向绑定\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      //改变当前页码触发\n      //this.currentPage = pageNum; 同上\n      this.load();\n    },\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code == '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","name","components","data","form","dialogVisible","search","currentPage","pageSize","total","tableData","created","load","methods","get","params","pageNum","then","res","records","add","save","id","put","code","$message","type","message","msg","post","handleEdit","row","JSON","parse","stringify","handleSizeChange","handleCurrentChange","handleDelete","console","log","delete"],"sources":["C:\\Users\\duxiangning\\Desktop\\Springboot+vue\\spring-vue-demo\\vue\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\" style=\"width: 100%;padding: 10px\">\n    <div style=\"margin:10px 0;\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-input clearable v-model=\"search\" placeholder=\"请输入内容\" style=\"width:20%;margin-left:10px;\"></el-input>\n      <el-button type=\"primary\" style=\"margin-left:10px;\" @click=\"load()\">查询</el-button>\n    </div>\n    <el-table :data=\"tableData\" border stripe style=\"width: 100%;\">\n      <el-table-column prop=\"id\" label=\"ID\" sortable />\n      <el-table-column prop=\"username\" label=\"用户名\" />\n      <el-table-column prop=\"nickName\" label=\"昵称\" />\n      <el-table-column prop=\"age\" label=\"年龄\" />\n      <el-table-column prop=\"sex\" label=\"性别\" />\n      <el-table-column prop=\"address\" label=\"地址\" />\n      <el-table-column fixed=\"right\" label=\"操作\" width=\"140\">\n        <template #default=\"scope\">\n          <el-button type=\"text\" size=\"small\" @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"你确定要删除吗?\" @confirm=\"handleDelete(scope.row.id)\">\n            <template #reference>\n              <el-button type=\"text\" size=\"small\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin:10px 0;\"></div>\n    <div class=\"demo-pagination-block\">\n      <el-pagination v-model:currentPage=\"currentPage\" v-model:page-size=\"pageSize\" :page-sizes=\"[5, 10, 20]\"\n        :small=\"small\" :disabled=\"disabled\" :background=\"background\" layout=\"total, sizes, prev, pager, next, jumper\"\n        :total=\"total\" @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" />\n      <el-dialog v-model=\"dialogVisible\" title=\"添加\" width=\"30%\">\n        <el-form :model=\"form\" label-width=\"120px\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" style=\"width:80%;\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\" style=\"width:80%;\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\" style=\"width:80%;\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio v-model=\"form.sex\" label=\"男\">男</el-radio>\n            <el-radio v-model=\"form.sex\" label=\"女\">女</el-radio>\n            <el-radio v-model=\"form.sex\" label=\"未知\">未知</el-radio>\n          </el-form-item>\n          <el-form-item label=\"地址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width:80%;\"></el-input>\n          </el-form-item>\n        </el-form>\n        <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取消</el-button>\n            <el-button type=\"primary\" @click=\"save()\">确定</el-button>\n          </span>\n        </template>\n      </el-dialog>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { request } from '@/utils/request';\n\nexport default {\n  name: 'HomeView',\n  components: {\n\n  },\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      search: \"\",\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: []\n    }\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        //console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      })\n    },\n    add() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n    save() {\n      if (this.form.id) {\n        request.put(\"/user\", this.form).then(res => {\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n        })\n      } else {\n        request.post(\"/user\", this.form).then(res => {\n          if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n        })\n      }\n      this.load();\n      this.dialogVisible = false;\n    },\n    handleEdit(row) {\n      //深拷贝，为了防止更改了本地变量里面的form\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n    handleSizeChange(pageSize) { //改变当前每页的个数触发\n      //this.pageSize = pageSize; 我使用了双向绑定\n      this.load();\n    },\n    handleCurrentChange(pageNum) {  //改变当前页码触发\n      //this.currentPage = pageNum; 同上\n      this.load();\n    },\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/user/\" + id).then(res => {\n        if (res.code == '0') {\n            this.$message({\n              type: \"success\",\n              message: \"删除成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            });\n          }\n      })\n      this.load();\n    }\n  }\n}\n</script>\n"],"mappings":"AA+DA,SAASA,OAAM,QAAS,iBAAiB;AAEzC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE,CAEZ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,CAAC,CAAC;MACRC,aAAa,EAAE,KAAK;MACpBC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE,CAAC;MACdC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,CAAC;MACRC,SAAS,EAAE;IACb;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC,CAAC;EACb,CAAC;EACDC,OAAO,EAAE;IACPD,IAAIA,CAAA,EAAG;MACLZ,OAAO,CAACc,GAAG,CAAC,OAAO,EAAE;QACnBC,MAAM,EAAE;UACNC,OAAO,EAAE,IAAI,CAACT,WAAW;UACzBC,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBF,MAAM,EAAE,IAAI,CAACA;QACf;MACF,CAAC,CAAC,CAACW,IAAI,CAACC,GAAE,IAAK;QACb;QACA,IAAI,CAACR,SAAQ,GAAIQ,GAAG,CAACf,IAAI,CAACgB,OAAO;QACjC,IAAI,CAACV,KAAI,GAAIS,GAAG,CAACf,IAAI,CAACM,KAAK;MAC7B,CAAC;IACH,CAAC;IACDW,GAAGA,CAAA,EAAG;MACJ,IAAI,CAACf,aAAY,GAAI,IAAI;MACzB,IAAI,CAACD,IAAG,GAAI,CAAC,CAAC;IAChB,CAAC;IACDiB,IAAIA,CAAA,EAAG;MACL,IAAI,IAAI,CAACjB,IAAI,CAACkB,EAAE,EAAE;QAChBtB,OAAO,CAACuB,GAAG,CAAC,OAAO,EAAE,IAAI,CAACnB,IAAI,CAAC,CAACa,IAAI,CAACC,GAAE,IAAK;UAC1C,IAAIA,GAAG,CAACM,IAAG,IAAK,GAAG,EAAE;YACnB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC,CAAC;UACJ,OAAO;YACL,IAAI,CAACF,QAAQ,CAAC;cACZC,IAAI,EAAE,OAAO;cACbC,OAAO,EAAET,GAAG,CAACU;YACf,CAAC,CAAC;UACJ;QACF,CAAC;MACH,OAAO;QACL5B,OAAO,CAAC6B,IAAI,CAAC,OAAO,EAAE,IAAI,CAACzB,IAAI,CAAC,CAACa,IAAI,CAACC,GAAE,IAAK;UAC3C,IAAIA,GAAG,CAACM,IAAG,IAAK,GAAG,EAAE;YACnB,IAAI,CAACC,QAAQ,CAAC;cACZC,IAAI,EAAE,SAAS;cACfC,OAAO,EAAE;YACX,CAAC,CAAC;UACJ,OAAO;YACL,I