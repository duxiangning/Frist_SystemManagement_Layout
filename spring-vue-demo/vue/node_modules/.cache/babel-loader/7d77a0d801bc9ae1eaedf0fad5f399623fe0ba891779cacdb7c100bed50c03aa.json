[\r\n          {required: true, message: '请输入密码', trigger: 'blur'}\r\n        ],\r\n        confirm: [\r\n          {required: true, message: '请确认密码', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    register(){\r\n      if(this.form.password != this.form.confirm){\r\n        this.$message({\r\n          type: \"error\",\r\n          message: '两次密码输入不一致!'\r\n        })\r\n        return\r\n      }\r\n      this.$refs['form'].validate((valid) => {\r\n        if(valid){\r\n          request.post(\"/user/register\",this.form).then(res =>{\r\n            if(res.code === '0'){\r\n              this.$message({\r\n                type: \"success\",\r\n                message: \"注册成功\"\r\n              })\r\n              this.$router.push(\"/\")  //登录成功之后 进行页面的跳转 , 跳转到主页\r\n            }else {\r\n              this.$message({\r\n                type: \"error\",\r\n                message: res.msg\r\n              })\r\n            }\r\n          })\r\n        }\r\n      })\r\n    }\r\n\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":";AA4CA,SAAQA,IAAI,EAAEC,MAAM,EAAEC,UAAU,QAAO,yBAAyB;AAChE,OAAOC,OAAM,MAAO,iBAAiB;AAErC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IACVL,IAAI;IACJC,MAAM;IACNC;EACF,CAAC;EAEDI,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,IAAI,EAAE,CAAC,CAAC;MACRC,KAAK,EAAE;QACLC,QAAQ,EAAE,CACR;UAACC,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAM,CAAC,CACrD;QACDC,QAAQ,EAAE,CACR;UAACH,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,EACnD;QACDE,OAAO,EAAE,CACP;UAACJ,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM;MAEtD;IACF;EACF,CAAC;EACDG,OAAO,EAAC;IACNC,QAAQA,CAAA,EAAE;MACR,IAAG,IAAI,CAACT,IAAI,CAACM,QAAO,IAAK,IAAI,CAACN,IAAI,CAACO,OAAO,EAAC;QACzC,IAAI,CAACG,QAAQ,CAAC;UACZC,IAAI,EAAE,OAAO;UACbP,OAAO,EAAE;QACX,CAAC;QACD;MACF;MACA,IAAI,CAACQ,KAAK,CAAC,MAAM,CAAC,CAACC,QAAQ,CAAEC,KAAK,IAAK;QACrC,IAAGA,KAAK,EAAC;UACPlB,OAAO,CAACmB,IAAI,CAAC,gBAAgB,EAAC,IAAI,CAACf,IAAI,CAAC,CAACgB,IAAI,CAACC,G